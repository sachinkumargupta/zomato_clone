 <table class="table table-striped table-responsive">
   <tr>
     <th>User Name</th>
     <th></th>
   </tr>
     <% @users.each do |user| %>
       <tr>
         <td><%= link_to user.name, user %></td>
         <td><% if current_user.admin? && !current_user?(user) %>
         <%= link_to "delete", user, method: :delete,
                                       data: { confirm: "You sure?" } %></td>
       </tr> 
     <% end %>
   <% end %>
 </table>
